{
  "version": 2,
  "buildCommand": "npm run ci:build",
  "outputDirectory": "dist",
  "framework": null,
  "installCommand": "npm ci",
  "env": {
    "NODE_VERSION": "24.x",
    "NODE_OPTIONS": "--enable-source-maps --no-warnings"
  },
  "functions": {
    "dist/index.js": {
      "memory": 256,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ]
}
